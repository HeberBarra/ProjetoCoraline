{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "rootDir": "./typescript/",
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false, 
    "outDir": "./javascript/",
    "removeComments": true,
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
